;;;; -*- Mode: Lisp; Syntax: ANSI-Common-Lisp; indent-tabs-mode: nil; coding: utf-8; show-trailing-whitespace: t -*-
;;
;; single phase pulse transformer
;; k =  2:1
;;

(netlist :name "fdb-sp-400"
         :elements-list (
(passive :name "D1"
                                  :class "conductance"
                                  :nodes-list ("N3" "N5")
                                  :model (model :name "simple-diode"
                                                :function-name "simple-diode-2"
                                                :class "function"
                                                :parameters-list (:vt 252d-3 :n 1d0 :is 1d-3)
                                                :probes-list ((probe :name "diode-a-1"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N3"))

                                                              (probe :name "diode-k-1"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N5")))))

                         (passive :name "D2"
                                  :class "conductance"
                                  :nodes-list ("N4" "N5")
                                  :model (model :name "simple-diode"
                                                :function-name "simple-diode-2"
                                                :class "function"
                                                :parameters-list (:vt 252d-3 :n 1d0 :is 1d-3)
                                                :probes-list ((probe :name "diode-a-2"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N4"))

                                                              (probe :name "diode-k-2"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N5")))))

                         (passive :name "D3"
                                  :class "conductance"
                                  :nodes-list ("GND" "N4")
                                  :model (model :name "simple-diode"
                                                :function-name "simple-diode-2"
                                                :class "function"
                                                :parameters-list (:vt 252d-3 :n 1d0 :is 1d-3)
                                                :probes-list ((probe :name "diode-a-3"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("GND"))

                                                              (probe :name "diode-k-3"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N4")))))

                         (passive :name "D4"
                                  :class "conductance"
                                  :nodes-list ("GND" "N3")
                                  :model (model :name "simple-diode"
                                                :function-name "simple-diode-2"
                                                :class "function"
                                                :parameters-list (:vt 252d-3 :n 1d0 :is 1d-3)
                                                :probes-list ((probe :name "diode-a-4"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("GND"))

                                                              (probe :name "diode-k-4"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N3")))))
