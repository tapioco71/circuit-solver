;;;;-*- mode: lisp -*-
(netlist :name "test-switch-12"
	 :elements-list ((source :name "V1"
		                 :class "voltage-source"
		                 :model (model :name "sinusoidal-voltage-1"
				               :function-name "sinusoidal-function"
				               :class "function"
				               :parameters-list (:amplitude 325d0
								 :frequency 50d0
								 :phase 0d0))
		                 :nodes-list ("n1" "gnd"))

			 (source :name "Vpulse1"
				 :class "voltage-source"
				 :nodes-list ("gnd" "nc1")
				 :model (model :name "pulse-voltage-1"
					       :function-name "impulse-function"
					       :class "function"
					       :parameters-list (:low-value 0d0
								 :high-value 1d0
								 :polarity t
								 :time-start 0.5d0
								 :time-end 15d0)))

			 (passive :name "S1"
				  :class "resistance"
				  :nodes-list ("n1" "n2")
				  :model (model :name "switch-1"
						:function-name "simple-switch-1"
						:class "function"
						:parameters-list (:lower-threshold 0.4
								  :upper-threshold 0.6
								  :on-resistance 0d0
								  :off-resistance 1d8)
						:probes-list ((probe :name "sp1"
								     :class "voltage-probe"
								     :nodes-list ("nc1"))
							      (probe :name "sp2"
								     :class "voltage-probe"
								     :nodes-list ("gnd")))
						:value 1d8))

			 (passive :name "R1"
				  :class "resistance"
				  :nodes-list ("n2" "n3")
				  :value 1d2)

			 (passive :name "D1"
				  :class "conductance"
				  :nodes-list ("n3" "n4")
				  :model (model :name "diode-1"
						:function-name "simple-diode-4"
						:class "function"
						:parameters-list (:maximum-forward-conductance 1d1
								  :maximum-backward-conductance 1d1
								  :minimum-conductance 0d0
								  :nf 1d1
                                                                  :nz 1d-1
								  :forward-voltage 1d0
								  :breakdown-voltage 8d2)
						:probes-list ((probe :name "diode-a-1"
								     :class "voltage-probe"
								     :nodes-list ("n3"))
							      (probe :name "diode-k-1"
								     :class "voltage-probe"
								     :nodes-list ("n4")))))

			 (passive :name "C1"
				  :class "capacitance"
				  :nodes-list ("n4" "gnd")
				  :value 6.8d-3)

			 (passive :name "D2"
				  :class "conductance"
				  :nodes-list ("gnd" "n4")
				  :model (model :name "diode-2"
						:function-name "simple-diode-4"
						:class "function"
						:parameters-list (:maximum-forward-conductance 1d1
								  :maximum-backward-conductance 1d1
								  :minimum-conductance 0d0
								  :nf 1d1
                                                                  :nz 1d-1
								  :forward-voltage 1d0
								  :breakdown-voltage 8d2)
						:probes-list ((probe :name "diode-a-2"
								     :class "voltage-probe"
								     :nodes-list ("gnd"))
							      (probe :name "diode-k-2"
								     :class "voltage-probe"
								     :nodes-list ("n4")))))

			 (passive :name "R2"
				  :class "resistance"
				  :nodes-list ("gnd" "n4")
				  :value 22d3)

			 (passive :name "L1"
				  :class "inductance"
				  :nodes-list ("n4" "n5")
				  :value 5.25d-3)

			 (passive :name "S2"
				  :class "conductance"
				  :nodes-list ("n5" "n6")
                                  :value 1d-6
				  :model (model :name "switch-2"
						:function-name "simple-switch-2"
						:class "function"
						:parameters-list (:on-time 20d0
								  :off-time 20.010d0
								  :on-conductance 33.3
								  :off-conductance 1d-6
                                                                  :k-on 1d3
                                                                  :k-off 1d3)))

			 (passive :name "D3"
				  :class "conductance"
				  :nodes-list ("n6" "n5")
				  :model (model :name "diode-3"
						:function-name "simple-diode-4"
						:class "function"
						:parameters-list (:maximum-forward-conductance 1d1
								  :maximum-backward-conductance 1d1
								  :minimum-conductance 0d0
								  :forward-voltage 1d0
								  :nf 1d1
                                                                  :nz 1d-1
								  :breakdown-voltage 9d2)
						:probes-list ((probe :name "diode-a-2"
								     :class "voltage-probe"
								     :nodes-list ("n6"))
							      (probe :name "diode-k-2"
								     :class "voltage-probe"
								     :nodes-list ("n5")))))
			 (passive :name "M1"
				  :class "conductance"
				  :nodes-list ("n6" "n5")
				  :model (model :name "mov-1"
						:function-name "simple-mov-2"
						:class "function"
						:parameters-list (:c 5d3
								  :beta 0.90d0)
						:probes-list ((probe :name "mov-1-1"
								     :class "voltage-probe"
								     :nodes-list ("n6"))
							      (probe :name "mov-1-2"
								     :class "voltage-probe"
								     :nodes-list ("n5")))))

			 (passive :name "L2"
				  :class "inductance"
				  :nodes-list ("n6" "n7")
				  :value 20d-3)

			 (passive :name "R3"
				  :class "resistance"
				  :nodes-list ("gnd" "n7")
				  :value 2.2d0)

			 (node :name "gnd"
			       :class "reference")

			 (node :name "n1"
			       :class "v-i")

			 (node :name "n2"
			       :class "v-i")

			 (node :name "n3"
			       :class "v-i")

			 (node :name "n4"
			       :class "v-i")

			 (node :name "n5"
			       :class "v-i")

			 (node :name "n6"
			       :class "v-i")

			 (node :name "n7"
			       :class "v-i")

			 (node :name "nc1"
			       :class "v-i")

			 (probe :name "current"
				:class "current-probe"
				:elements-list ("D1" "D2" "D3" "R1" "R2" "R3" "L1" "M1"))

			 (probe :name "voltage"
				:class "voltage-probe"
				:nodes-list ("n1" "n2" "n3" "n4" "n5" "n6" "nc1"))))
