				  :class "capacitance"
				  :nodes-list ("gnd" "n1")
				  :value 6.8d-3)

			 (passive :name "R1"
				  :class "resistance"
				  :nodes-list ("n1" "gnd")
				  :value 20d3)

			 (passive :name "L1"
				  :class "inductance"
				  :nodes-list ("n1" "n2")
				  :value 5.25d-3)

			 (passive :name "RL1"
