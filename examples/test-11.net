;;;; -*- Mode: Lisp; Syntax: ANSI-Common-Lisp; indent-tabs-mode: nil; coding: utf-8; show-trailing-whitespace: t -*-
(netlist :name "test-11"
         :elements-list ((source :name "V1"
                                 :class "voltage-source"
                                 :nodes-list ("N1" "GND")
                                 :model (model :name "sinusoidal-voltage"
                                               :function-name "sinusoidal-function"
                                               :class "function"
                                               :parameters-list (:amplitude 2.5d-1 :offset 10d0 :frequency 50d0 :phase 0d0)
                                               :value 0d0))

                         ;; (source :name "V1"
		         ;;         :class "voltage-source"
		         ;;         :nodes-list ("N1" "GND")
		         ;;         :model (model :name "ramp"
			 ;;                       :function-name "ramp-function"
			 ;;                       :class "function"
			 ;;                       :parameters-list (:t0 0d0 :m 1d3)))

                         (passive :name "L1"
                                  :class "inductance"
                                  :nodes-list ("N1" "N2")
                                  :value 2.2d-6)

                         (passive :name "R1"
                                  :class "resistance"
                                  :nodes-list ("N2" "N3")
                                  :value 2.2d2)

                         (passive :name "C1"
                                  :class "capacitance"
                                  :nodes-list ("GND" "N3")
                                  :value 10d-6)

                         (passive :name "D1"
                                  :class "conductance"
                                  :nodes-list ("GND" "N3")
                                  :model (model :name "simple-diode"
                                                :function-name "zener-diode-1"
                                                :class "function"
                                                :parameters-list (:kf 1d5 :nf 1d2 :kz 1d6 :nz 1d2 :vf 0.7d0 :vz 5.6d0)
                                                :probes-list ((probe :name "diode-a-5"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("GND"))

                                                              (probe :name "diode-k-5"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N3")))))

		         (node :name "GND"
		               :class "reference")

		         (node :name "N1"
		               :class "v-i")

		         (node :name "N2"
		               :class "v-i")

                         (node :name "N3"
		               :class "v-i")

                         (probe :name "currents"
                                :class "current-probe"
                                :elements-list ("V1" "R1" "L1" "C1" "D1"))

		         (probe :name "voltages"
		                :class "voltage-probe"
		                :nodes-list ("N1" "N2" "N3"))))
