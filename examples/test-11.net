;;;; -*- Mode: Lisp; Syntax: ANSI-Common-Lisp; indent-tabs-mode: nil; coding: utf-8; show-trailing-whitespace: t -*-
(netlist :name "test-11"
         :elements-list ((source :name "V1"
                                 :class "voltage-source"
                                 :nodes-list ("N1" "GND")
                                 :model (model :name "sinusoidal-voltage"
                                               :function-name "sinusoidal-function"
                                               :class "function"
                                               :parameters-list (:amplitude 5d-1 :offset 10d0 :frequency 50d0 :phase 0d0)
                                               :value 0d0))

                         (passive :name "L1"
                                  :class "inductance"
                                  :nodes-list ("N1" "N2")
                                  :value 2.2d-6)

                         (passive :name "R1"
                                  :class "resistance"
                                  :nodes-list ("N2" "N3")
                                  :value 2.2d2)

                         (passive :name "C1"
                                  :class "capacitance"
                                  :nodes-list ("GND" "N3")
                                  :value 10d-6)

                         (passive :name "D1"
                                  :class "conductance"
                                  :nodes-list ("GND" "N3")
                                  :model (model :name "simple-diode"
                                                :function-name "zener-diode"
                                                :class "function"
                                                :parameters-list (:minimum-conductance 1d-6
                                                                  :maximum-forward-conductance 1d4
                                                                  :maximum-backward-conductance 2d4
                                                                  :nf 1d2
                                                                  :nz 1d2
                                                                  :forward-voltage 0.7d0
                                                                  :breakdown-voltage 6.8d0)
                                                :probes-list ((probe :name "diode-a-5"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("GND"))

                                                              (probe :name "diode-k-5"
                                                                     :class "voltage-probe"
                                                                     :nodes-list ("N3")))))

		         (node :name "GND"
		               :class "reference")

		         (node :name "N1"
		               :class "v-i")

		         (node :name "N2"
		               :class "v-i")

                         (node :name "N3"
		               :class "v-i")

                         (probe :name "currents"
                                :class "current-probe"
                                :elements-list ("V1" "R1" "L1" "C1" "D1"))

		         (probe :name "voltages"
		                :class "voltage-probe"
		                :nodes-list ("N1" "N2" "N3"))))
