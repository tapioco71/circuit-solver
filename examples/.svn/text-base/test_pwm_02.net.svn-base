(NETLIST
	:NAME "pwm_test"
	:ELEMENTS-LIST ((SOURCE 
				:NAME "V1"
				:CLASS "voltage-source"
				:NODES-LIST ("GND" "N1")
				:MODEL (MODEL :NAME "square-voltage-1"
					:FUNCTION-NAME "square-function"
					:CLASS "function"
					:PARAMETERS-LIST (:LOW-VALUE 0d0 :HIGH-VALUE 10d0 :PERIOD 2d-4 :T0 0d0 :DUTY-CYCLE 0.5d0 :RISING-TIME 0d0 :FALLING-TIME 0d0 :POLARITY t)))
					      		  
			(PASSIVE
				:NAME "R1"
				:CLASS "resistance"
				:NODES-LIST ("N1" "N2")
				:VALUE 6.8d0)

			(PASSIVE
				:NAME "L1"
				:CLASS "inductance"
				:NODES-LIST ("N2" "GND")
				:VALUE 22d-6)

							
			(NODE
				:NAME "GND"
				:CLASS "reference")

			(NODE
				:NAME "N1"
				:CLASS "v-i")

			(NODE
				:NAME "N2"
				:CLASS "v-i")
			
			(PROBE
			       :NAME "resistance-current"
			       :CLASS "current-probe"
			       :ELEMENTS-LIST ("R1"))

			(PROBE
			       :NAME "resistance-voltage"
			       :CLASS "voltage-probe"
			       :NODES-LIST ("N1" "N2"))))