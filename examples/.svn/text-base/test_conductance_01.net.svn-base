(NETLIST
	:NAME "test_conductance_01"
	:ELEMENTS-LIST ((SOURCE
				:NAME "V1"
				:CLASS "voltage-source"
				:NODES-LIST ("GND" "N1")
				:VALUE 12d0)

			(PASSIVE
				:NAME "R1"
				:CLASS "resistance"
				:NODES-LIST ("N1" "N2")
				:VALUE 1d3)

			(PASSIVE
				:NAME "G1"
				:CLASS "conductance"
				:NODES-LIST ("N2" "GND")
				:VALUE 1d-3)					      		  
							
			(NODE
				:NAME "GND"
				:CLASS "reference")

			(NODE
				:NAME "N1"
				:CLASS "v-i")

			(NODE
				:NAME "N2"
				:CLASS "v-i")

			(PROBE
			       :NAME "current"
			       :CLASS "current-probe"
			       :ELEMENTS-LIST ("R1"))
			(PROBE
			       :NAME "voltage"
			       :CLASS "voltage-probe"
			       :NODES-LIST ("N2"))))