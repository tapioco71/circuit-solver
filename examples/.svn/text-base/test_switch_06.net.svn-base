(NETLIST
	:NAME "switch_test_06"
	:ELEMENTS-LIST ((SOURCE
				:NAME "V1"
				:CLASS "voltage-source"
				:NODES-LIST ("GND" "N1")
				:VALUE 2.4d0)

			(SOURCE 
				:NAME "V2"
				:CLASS "voltage-source"
				:NODES-LIST ("GND" "Nc")
				:MODEL (MODEL :NAME "square-voltage-1"
					:FUNCTION-NAME "square-function-2"
					:CLASS "function"
					:PARAMETERS-LIST (:LOW-VALUE 0d0 :HIGH-VALUE 10d0 :PERIOD 3.33d-6 :T0 0d0 :DUTY-CYCLE 0.5d0 :RISING-TIME 0d0 :FALLING-TIME 0d0 :POLARITY nil)))

			(PASSIVE
				:NAME "L1"
				:CLASS "inductance"
				:NODES-LIST ("N1" "N2")
				:VALUE 22d-6)

			(PASSIVE
				:NAME "S1"
				:CLASS "conductance"
				:NODES-LIST ("GND" "N2")
				:MODEL (MODEL 
				       	      :NAME "simple-switch-2"
					      :FUNCTION-NAME "simple-switch-2"
					      :CLASS "function"
					      :PARAMETERS-LIST (:UPPER-THRESHOLD 6d0 :LOWER-THRESHOLD 4d0 :ON-CONDUCTANCE 1d3 :OFF-CONDUCTANCE 1d-3 :ON-TIME 1d-8 :OFF-TIME 1d-8) 
					      :PROBES-LIST ((PROBE
									:NAME "Sp1"
									:CLASS "voltage-probe"
									:NODES-LIST ("GND"))
							
							    (PROBE
									:NAME "Sp2"
									:CLASS "voltage-probe"
									:NODES-LIST ("Nc")))
					      :VALUE 1d-3))

;			(PASSIVE
;				:NAME "Ds1"
;				:CLASS "conductance"
;				:NODES-LIST ("GND" "N2")
;				:MODEL (MODEL :NAME "simple-diode-1"
;				       	      :FUNCTION-NAME "simple-diode-4"
;					      :CLASS "function"
;					      :PARAMETERS-LIST (:VT 252d-3 :N 1d0 :IS 1d-6 :ORDER 1)
;					      :PROBES-LIST ((PROBE 
;					      		   	   :NAME "diode-a-s1"
;								   :CLASS "voltage-probe"
;								   :NODES-LIST ("GND"))
;
;							    (PROBE 
;					      		   	   :NAME "diode-k-s1"
;								   :CLASS "voltage-probe"
;								   :NODES-LIST ("N2")))	
;					      :VALUE 0d0))

			(PASSIVE
				:NAME "D1"
				:CLASS "conductance"
				:NODES-LIST ("N2" "N3")
				:MODEL (MODEL :NAME "simple-diode-1"
				       	      :FUNCTION-NAME "simple-diode-4"
					      :CLASS "function"
					      :PARAMETERS-LIST (:VT 252d-3 :N 1d0 :IS 1d-8 :ORDER 1)
					      :PROBES-LIST ((PROBE 
					      		   	   :NAME "diode-a-1"
								   :CLASS "voltage-probe"
								   :NODES-LIST ("N2"))

							    (PROBE 
					      		   	   :NAME "diode-k-1"
								   :CLASS "voltage-probe"
								   :NODES-LIST ("N3")))	
					      :VALUE 0d0))
					      		  
			(PASSIVE
				:NAME "Rload"
				:CLASS "resistance"
				:NODES-LIST ("N3" "GND")
				:VALUE 5d0)

			(PASSIVE
				:NAME "Cd"
				:CLASS "capacitance"
				:NODES-LIST ("N3" "GND")
				:VALUE 330d-6)

			(PASSIVE
				:NAME "Rs"
				:CLASS "resistance"
				:NODES-LIST ("Nc" "GND")
				:VALUE 10d0)
							
			(NODE
				:NAME "GND"
				:CLASS "reference")

			(NODE
				:NAME "N1"
				:CLASS "v-i")

			(NODE
				:NAME "N2"
				:CLASS "v-i")

			(NODE
				:NAME "N3"
				:CLASS "v-i")

			(NODE
				:NAME "Nc"
				:CLASS "v-i")
			
;			(INITIAL-CONDITION
;						:NAME "capacitance-voltage"
