;						:VALUE 48d0)

			(PROBE
			       :NAME "current"
			       :CLASS "current-probe"
			       :ELEMENTS-LIST ("Rload" "L1" "S1"))
			(PROBE
			       :NAME "voltage"
			       :CLASS "voltage-probe"
			       :NODES-LIST ("Nc" "N1" "N2" "N3"))))