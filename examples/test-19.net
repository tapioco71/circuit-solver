(netlist :name "test-19"
         :elements-list ((source :name "V1"
		                 :class "voltage-source"
		                 :nodes-list ("N1" "GND")
		                 :model (model :name "sinusoidal"
				               :function-name "sinusoidal-function"
				               :class "function"
				               :parameters-list (:amplitude 240d0 :frequency 50d0)))

                         (passive :name "R1"
                                  :class "resistance"
                                  :nodes-list ("N1" "N2")
                                  :value 2.2d-3)

                         (subcircuit :name "DB1"
