		         (node :name "GND"
		               :class "reference")

		         (node :name "N1"
		               :class "v-i")

		         (node :name "N2"
		               :class "v-i")

                         (node :name "N3"
		               :class "v-i")

		         (node :name "N4"
		               :class "v-i")

                         (probe :name "currents"
                                :class "current-probe"
                                :elements-list ("V1" "R1" "R2" "C1"))

		         (probe :name "voltages"
		                :class "voltage-probe"
		                :nodes-list ("N1" "N2" "N3" "N4"))))
