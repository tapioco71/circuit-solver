;;
;; pay attention to three-phase machine abduction phenomena
;;

(NETLIST
 :NAME "test_1ph_fault_02"
 :ELEMENTS-LIST ((SOURCE 
		  :NAME "V1"
		  :CLASS "voltage-source"
		  :NODES-LIST ("GND" "N1")
		  :MODEL (MODEL 
			  :NAME "sinusoidal-voltage-1"
			  :FUNCTION-NAME "sinusoidal-function"
			  :CLASS "function"
			  :PARAMETERS-LIST (:AMPLITUDE 311d0 :FREQUENCY 50d0 :PHASE 0d0)))

		 (SOURCE
		  :NAME "Vb"
		  :CLASS "voltage-source"
		  :NODES-LIST ("N10" "GND")
		  :MODEL (MODEL
			  :NAME "square-voltage-1"
			  :FUNCTION-NAME "bistable-function-1"
			  :CLASS "function"
			  :PARAMETERS-LIST (:POLARITY nil :T0 200d-3 :LOW-VALUE 0d0 :HIGH-VALUE 10d0)))

		 (PASSIVE
		  :NAME "Rb"
		  :CLASS "resistance"
		  :NODES-LIST ("N10" "GND")
		  :VALUE 1d6)

		 (PASSIVE
		  :NAME "Rload"
		  :CLASS "resistance"
		  :NODES-LIST ("N2" "N3")
		  :VALUE 7d0)

		 (PASSIVE
		  :NAME "Lload"
		  :CLASS "inductance"
		  :NODES-LIST ("N3" "N4")
		  :VALUE 350d-3)

		 (PASSIVE
		  :NAME "C1"
		  :CLASS "capacitance"
		  :NODES-LIST ("N2" "GND")
		  :VALUE 10e-6)

		 (PASSIVE
		  :NAME "G1"
		  :CLASS "conductance"
		  :NODES-LIST ("N4" "GND")
		  :MODEL (MODEL
			  :NAME "gas-discharge-lamp-1"
			  :FUNCTION-NAME "gas-discharge-lamp"
			  :CLASS "differential"
			  :PARAMETERS-LIST (:A2 1.12d0 :A1 0d0 :B3 0d0 :B2 1074d0 :B1 3778d0 :B0 0d0 :FRAG 1d0 :EQE 0d0)
			  :PROBES-LIST ((PROBE 
					 :NAME "lamp-current-1"
					 :CLASS "current-probe"
					 :ELEMENTS-LIST ("G1")))
			  :VALUE 1d-3))

		 (PASSIVE
		  :NAME "I1"
		  :CLASS "conductance"
		  :NODES-LIST ("N1" "N2")
		  :MODEL (MODEL
			  :NAME "breaker"
			  :FUNCTION-NAME "simple-switch-2"
			  :CLASS "function"
			  :PARAMETERS-LIST (:ON-CONDUCTANCE 1d3 :OFF-CONDUCTANCE 1d-12 :ON-TIME 1d-6 :OFF-TIME 1d-6 :UPPER-THRESHOLD 6d0 :LOWER-THRESHOLD 4d0)
			  :PROBES-LIST ((PROBE
					 :NAME "P1"
					 :CLASS "voltage-probe"
					 :NODES-LIST ("GND"))
					(PROBE
					 :NAME "P2"
					 :CLASS "voltage-probe"
					 :NODES-LIST ("N10")))
			  :VALUE 1d-12))


		 (NODE
		  :NAME "GND"
		  :CLASS "reference")

		 (NODE
		  :NAME "N1"
		  :CLASS "v-i")

		 (NODE
		  :NAME "N2"
		  :CLASS "v-i")

		 (NODE
		  :NAME "N3"
		  :CLASS "v-i")

		 (NODE
		  :NAME "N4"
		  :CLASS "v-i")

		 (NODE
		  :NAME "N10"
		  :CLASS "v-i")

		 (PROBE
		  :NAME "current"
		  :CLASS "current-probe"
		  :ELEMENTS-LIST ("V1" "Rload" "Lload" "C1" "G1" "I1"))

		 (PROBE
		  :NAME "voltage"
		  :CLASS "voltage-probe"
		  :NODES-LIST ("N1" "N2" "N3" "N4" "N10"))))