(netlist :name "ctype-filter-001"
         :elements-list ((source :name "V1"
		                 :class "voltage-source"
		                 :nodes-list ("N1" "N")
		                 :model (model :name "harmonic"
				               :function-name "harmonic-function"
				               :class "function"
